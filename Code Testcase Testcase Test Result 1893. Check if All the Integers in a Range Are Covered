bool isCovered(int** ranges, int rangesSize, int* rangesColSize, int left, int right) {
    int diff[52] = {0};  
    for (int i = 0; i < rangesSize; i++) {
        int start = ranges[i][0];
        int end   = ranges[i][1];
        diff[start] += 1;
        if (end + 1 < 52) {
            diff[end + 1] -= 1;
        }
    }
    int cover = 0;
    for (int num = 1; num <= 50; num++) {
        cover += diff[num];  
        if (num >= left && num <= right && cover <= 0) {
            return false;
        }
    }
    return true;
}
